
echo ls :

./ft_ls ../../ | grep -c "**" > my_ls 
ls ../../ | grep -c "**" > ls
diff my_ls ls
rm my_ls ls 

echo ls -l :

./ft_ls -l ../../ | grep -c "**" > my_ls 
ls -l ../../ | grep -c "**" > ls
diff my_ls ls
rm my_ls ls 

